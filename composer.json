{
  "name": "srag/piechart",
  "description": "ILIAS Pie Chart UI Component",
  "version": "1.1.0",
  "extra": {
    "ilias_plugin": {
      "ilias_min_version": "5.4.0",
      "ilias_max_version": "6.999"
    },
    "generate_plugin_readme_template": "SRAG_ILIAS_LIBRARY"
  },
  "type": "library",
  "keywords": [
    "ILIAS",
    "Pie",
    "Chart",
    "UI",
    "Component"
  ],
  "homepage": "https://github.com/studer-raimann/PieChart",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "studer + raimann ag - Team Custom 1",
      "email": "support-custom1@studer-raimann.ch",
      "homepage": "https://studer-raimann.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://plugins.studer-raimann.ch/goto.php?target=uihk_srsu_LPIECH"
  },
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "srag/generateplugininfoshelper": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\PieChart\\": "src/"
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "pre-autoload-dump": [
      "srag\\GeneratePluginInfosHelper\\GeneratePluginReadme::generatePluginReadme"
    ]
  }
}